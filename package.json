    "axios": "0.19.2",
    "bootstrap": "4.4.1",
    "chai": "4.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "jquery": "3.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.3.0",
    "redux": "4.0.5",
    "rxjs": "6.5.5",
    "sinon": "9.0.2"
  },
  "devDependencies": {
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
